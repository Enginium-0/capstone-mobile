{
  "expo": {
    "name": "capstone-mobile",
    "slug": "capstone-mobile",
    "owner": "enginium_0",
    "scheme": "facilitease",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/emblem.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/emblem.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/emblem.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.enginium_0.capstonemobile"
    },
    "web": {
      "favicon": "./assets/emblem.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-secure-store"
    ]
  }
}
