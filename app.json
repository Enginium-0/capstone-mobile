{
  "expo": {
    "name": "FacilitEASE",
    "slug": "facilitease-mobile",
    "owner": "enginium_0",
    "scheme": "facilitease",
    "version": "1.0.0",
    "originalFullName": "@enginium_0/facilitease-mobile",
    "orientation": "portrait",
    "icon": "./assets/emblem.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/emblem.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/emblem.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "package": "com.enginium_0.capstonemobile"
    },
    "web": {
        "bundler": "metro",
        "output": "server",
        "favicon": "./assets/emblem.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-secure-store",
      "@react-native-google-signin/google-signin"
    ],
    "extra": {
      "originalFullName": "@enginium_0/facilitease-mobile"
    }
  }
}
